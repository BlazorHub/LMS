@model LogoutViewModel
@{
    ViewData["Title"] = "登出";
}

<div class="logout-page">
    <div class="lead">
        <h1>登出</h1>
        <p>你确定要登出吗？</p>
    </div>

    <form asp-action="Logout">
        <input type="hidden" name="logoutId" value="@Model.LogoutId" />
        <div class="form-group">
            <button class="btn btn-primary">确定</button>
        </div>
    </form>
</div>
